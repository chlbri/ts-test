    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "edit-json-file": "^1.6.2",
    "eslint": "^8.3.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
